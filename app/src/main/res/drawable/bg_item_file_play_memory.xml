    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_bg_play_audio_icon_play_audio_memory" />
            <corners android:radius="@dimen/_2sdp" />
        </shape>
    </item>
</selector>